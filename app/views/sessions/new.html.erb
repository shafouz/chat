<h1>Sign In</h1>

<p id="notice"><%= notice %></p>
<p id="alert"><%= alert %></p>

<%= form_with url: sign_in_path do |f| %>
  <p>
  <%= f.label 'email:'%><br>
  <%= f.text_field :email %>
  </p>
  <p>
  <%= f.label 'password:'%><br>
  <%= f.password_field :password %>
  </p>
  <p>
  <%= f.submit 'Log In' %>
  </p>
<% end %>

<%= link_to "Register", new_user_path %>
